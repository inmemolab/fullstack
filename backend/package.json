{
  "name": "lab-fullstack-backend",
  "version": "1.0.0",
  "description": "Para Proyectos",
  "main": "./src/server.ts",
  "keywords": [
    "node",
    "express",
    "typescript",
    "backend",
    "sequelize",
    "mysql",
    "socket"
  ],
  "author": {
    "name": "EnMemoLab",
    "email": "enmemolab@gmail.com",
    "url": "https://enmemolab.com"
  },
  "license": "ISC",
  "scripts": {
    "server": "nodemon --config nodemon src/server.ts",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "yarn eslint --cache --max-warnings 0 --report-unused-disable-directives \"**/*.{js,jsx,ts,tsx}\"",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "lint:prettier": "yarn prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint:prettier:fix": "yarn prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint:lint-staged": "lint-staged",
    "test": "jest --coverage",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
