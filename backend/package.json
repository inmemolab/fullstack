    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.0",
    "debug": "^4.3.4",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-validator": "^6.14.0",
    "geoip-lite": "^1.4.4",
    "helmet": "^5.0.2",
    "i18n": "^0.14.2",
    "ip": "^1.1.5",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.2",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "node-cron": "^3.0.0",
    "nodemailer": "^6.7.3",
    "nodemailer-markdown": "^1.0.3",
    "sequelize": "^6.18.0",
    "socket.io": "^4.4.1",
    "uuid": "^8.3.2",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.6.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
