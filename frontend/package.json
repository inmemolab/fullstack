{
  "name": "lab-fullstack-frontend",
  "private": true,
  "version": "1.1.2",
  "description": "For futures apps",
  "author": {
    "name": "EnMemoLab",
    "email": "enmemolab@gmail.com",
    "url": "https://enmemolab.com"
  },
  "keywords": [
    "vite",
    "vue3",
    "typescript"
  ],
  "scripts": {
    "dev": "ts-node server",
    "server": "cross-env NODE_ENV=production ts-node server",
    "build": "yarn build:client && yarn build:server",
    "preview": "npx http-server dist/static",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "build:ssg": "vite build --ssrManifest --outDir dist/static && yarn build:server && npx ts-node prerender",
    "typecheck": "vue-tsc --noEmit",
    "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:stylelint",
    "lint:eslint": "yarn eslint --cache --max-warnings 0 --report-unused-disable-directives \"**/*.{js,jsx,ts,tsx,vue,html}\"",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "lint:prettier": "yarn prettier --check \"**/*.{js,jsx,ts,tsx,vue,html,json,md}\"",
    "lint:prettier:fix": "yarn prettier --write \"**/*.{js,jsx,ts,tsx,vue,html,json,md}\"",
    "lint:stylelint": "yarn stylelint --cache -q \"**/*.{css,scss,vue}\"",
    "lint:stylelint:fix": "yarn lint:stylelint --fix",
    "lint:lint-staged": "lint-staged",
    "test": "jest --coverage",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/vue-fontawesome": "^3.0.0-5",
    "@fullcalendar/core": "^5.10.1",
    "@fullcalendar/daygrid": "^5.10.1",
    "@fullcalendar/interaction": "^5.10.1",
    "@fullcalendar/timegrid": "^5.10.1",
    "@fullcalendar/vue3": "^5.10.1",
    "@popperjs/core": "^2.11.4",
    "@vueuse/core": "^8.2.6",
    "@vueuse/head": "^0.7.6",
    "@vueuse/motion": "^2.0.0-beta.12",
    "await-to-js": "^3.0.0",
    "axios": "^0.26.1",
    "bootstrap": "^5.1.3",
    "express": "^4.17.3",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.2",
    "moment-timezone": "^0.5.34",
    "pinia": "^2.0.13",
    "pinia-plugin-persist": "^1.0.0",
    "socket.io-client": "^4.4.1",
    "ua-parser-js": "^1.0.2",
    "uuid": "^8.3.2",
    "vee-validate": "^4.5.10",
    "vue": "^3.2.33",
    "vue-i18n": "^9.2.0-beta.30",
    "vue-router": "^4.0.13",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^2.2.2",
    "@cypress/vue": "^3.1.1",
    "@intlify/vite-plugin-vue-i18n": "^3.4.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.24",
    "@types/serve-static": "^1.13.10",
    "@types/ua-parser-js": "^0.7.36",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/compiler-sfc": "^3.2.33",
    "@vue/test-utils": "^2.0.0-rc.20",
    "autoprefixer": "^10.4.4",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "cypress": "^9.5.4",
    "dotenv": "^16.0.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.6.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^1.0.0",
    "lint-staged": "^12.3.8",
    "postcss": "^8.4.12",
    "postcss-html": "^1.4.1",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "sass": "^1.50.0",
    "serve-static": "^1.15.0",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.7.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.2.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.5",
    "vue-tsc": "^0.34.6"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.vue": [
      "prettier --write",
      "eslint --fix",
      "stylelint --fix",
      "git add"
    ],
    "*.{scss,less,styl,html}": [
      "stylelint --fix",
      "prettier --write",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inmemolab/lab-fullstack-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/inmemolab/lab-fullstack-frontend/issues"
  },
  "homepage": "https://github.com/inmemolab/lab-fullstack-frontend#readme",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  }
}
